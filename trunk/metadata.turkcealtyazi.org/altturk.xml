<?xml version="1.0" encoding="iso-8859-9"?><scraper framework="1" date="2010-10-08" name="New Scraper" content="movies" thumb="icon.png" language="tr">
	<CreateSearchUrl SearchStringEncoding="utf-8" dest="3">
		<RegExp input="$$2" output="&lt;url&gt;http://www.turkcealtyazi.org/find.php?cat=mov&amp;find=\1&lt;/url&gt;" dest="3">
			<RegExp input="$$1" output="\1" dest="2">
				<expression repeat="yes" noclean="1" />
			</RegExp>
			<expression />
		</RegExp>
	</CreateSearchUrl>
	<GetSearchResults dest="8">
		<RegExp input="$$5" output="&lt;?xml version=&quot;1.0&quot; encoding=&quot;iso-8859-9&quot; standalone=&quot;yes&quot;?&gt;&lt;results&gt;\1&lt;/results&gt;" dest="8">
			<!--http://www.turkcealtyazi.org/mov/0055719/the-300-spartans.html-->
			<RegExp input="$$1" output="&lt;entity&gt;&lt;title&gt;\3 /&lt;/title&gt;&lt;thumb&gt;http://www.turkcealtyazi.org/film/mthumb/\1&lt;/thumb&gt;&lt;url&gt;http://www.turkcealtyazi.org\2&lt;/url&gt;&lt;id&gt;\3&lt;/id&gt;&lt;year&gt;\4&lt;/year&gt;&lt;/entity&gt; \n" dest="5">
				<expression cs="true" repeat="yes"> src="/film/mthumb/(.*?)"&gt;&lt;/td&gt;&lt;td class='row1'&gt;&lt;a href='(.*?)'&gt;&lt;b&gt;(.*?)&lt;/b&gt;&lt;/a&gt;&lt;br /&gt;\((.*?)\)&lt;/td&gt;</expression>
			</RegExp>
			<expression noclean="1" />
		</RegExp>
	</GetSearchResults>
	<GetDetails dest="3">
		<RegExp input="$$5" output="&lt;details&gt;\1&lt;/details&gt;" dest="3">
			<!--Title-->
			<RegExp input="$$1" output="&lt;title&gt;\1&lt;/title&gt;" dest="5">
				<expression>&lt;title&gt;(.[^\(]*)(.*?)\(</expression>
			</RegExp>
			<!--Year Film-->
			<RegExp input="$$1" output="&lt;year&gt;\2&lt;/year&gt;" dest="5+">
				<expression>"&gt;&lt;a href="\/filtre.php\?yil=(.*?)"&gt;([0-9]*)&lt;/a</expression>
			</RegExp>
			<!--rating-->
			<RegExp input="$$1" output="&lt;rating&gt;\1 puan&lt;/rating&gt;" dest="5+">
				<expression>&lt;strong&gt;(.*?)&lt;/strong&gt;&lt;/span&gt;&lt;span style="color:#666;font-size:9px;"&gt; (.*?)&lt;/span&gt;</expression>
			</RegExp>
			<!--Director-->
			<RegExp input="$$1" output="&lt;director&gt;\2&lt;/director&gt;" dest="5+">
				<expression noclean="1">netmen:&lt;/div&gt;\s*&lt;div class="right-container2"&gt;\s*&lt;a href="/prs(.*?)="(.*?)"&gt;</expression>
			</RegExp>
			<!--Runtime Film-->
			<RegExp input="$$1" output="&lt;runtime&gt;\1&lt;/runtime&gt;" dest="5+">
				<expression>&lt;div class="left-container2"&gt;S&#xFFFD;re:&lt;/div&gt;\s*&lt;div class="right-container2"&gt;(.*?)&lt;/div&gt;</expression>
			</RegExp>
			<!--Genre Film-->
			<RegExp input="$$1" output="&lt;genre&gt;\2&lt;/genre&gt;" dest="5+">
				<expression repeat="yes">&lt;a href="/filtre.php\?tur(.[^&gt;]*)&gt;(.*?)&lt;</expression>
			</RegExp>
			<!--Thumbnail-->
			<RegExp input="$$1" output="&lt;thumb&gt;http://www.turkcealtyazi.org/images/poster/\1.jpg&lt;/thumb&gt;" dest="5+">
				<expression noclean="1">&lt;link rel="image_src" href="http://www.turkcealtyazi.org/film/images/(.*?).jpg</expression>
			</RegExp>
			<!--Actors-->
			<RegExp input="$$1" output="&lt;actor&gt;&lt;name&gt;\2&lt;/name&gt;&lt;thumb&gt;http://www.turkcealtyazi.org/\1&lt;/thumb&gt;&lt;/actor&gt;" dest="5+">
				<expression repeat="yes" trim="1,3,5">l"&gt;&lt;img src="/film/images/(.*?)" class="resim2" border="0" width="100" height="140" alt="(.*?)"</expression>
			</RegExp>
			<!--info-->
			<RegExp input="$$1" output="&lt;plot&gt;\1&lt;/plot&gt;" dest="5+">
				<expression trim="1">&lt;div id="contextual" style="padding:4px;font: 12px verdana;color: #666;"&gt;(.*?)&lt;div style="</expression>
			</RegExp>
			<!--Fanart-->
			<RegExp conditional="fanart" input="$$1" output="&lt;url function=\1&lt;/url&gt;" dest="5+">
				<expression>eksisozluk" /&gt;&lt;/a&gt;\s*&lt;a href="(.*?)l" rel="nofollow</expression>
			</RegExp>
			<expression noclean="1" />
		</RegExp>
	</GetDetails>
</scraper>